{
  "name": "create-electron-ts-react",
  "productName": "electron-ts-react",
  "version": "2.0.1",
  "description": "My Electron application description",
  "repository": {
    "type": "git",
    "url": "https://github.com/saostad/Electron-React-Typescript.git"
  },
  "bin": "./setup.js",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "TFM",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron-ts-react"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/renderer/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@babel/core": "^7.7.7",
    "@hot-loader/react-dom": "^16.11.0",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-upload-client": "^12.1.0",
    "chart.js": "2.7.3",
    "classnames": "2.2.6",
    "core-js": "^3.6.4",
    "decompress-zip": "^0.2.2",
    "dotenv": "^8.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "file-loader": "^5.0.2",
    "graphql": "^14.5.8",
    "history": "4.7.2",
    "image-size": "^0.8.3",
    "jimp": "^0.9.3",
    "js-beautify": "^1.10.3",
    "moment": "2.23.0",
    "monaco-editor": "^0.19.1",
    "node-sass": "4.12.0",
    "nouislider": "12.1.0",
    "perfect-scrollbar": "1.4.0",
    "prop-types": "15.6.2",
    "react": "^16.12.0",
    "react-big-calendar": "0.20.2",
    "react-bootstrap-sweetalert": "4.4.1",
    "react-bootstrap-switch": "15.5.3",
    "react-bootstrap-wizard": "0.0.7",
    "react-chartjs-2": "2.7.4",
    "react-datetime": "2.16.3",
    "react-dom": "^16.12.0",
    "react-google-maps": "9.4.5",
    "react-jvectormap": "0.0.4",
    "react-monaco-editor": "^0.32.1",
    "react-notification-alert": "0.0.9",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.3",
    "react-select": "2.2.0",
    "react-table": "6.8.6",
    "react-tagsinput": "3.19.0",
    "reactstrap": "7.0.2",
    "regenerator-runtime": "^0.13.3",
    "url-loader": "^3.0.0"
  },
  "devDependencies": {
    "electron-prebuilt-compile": "1.7.9",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@electron-forge/cli": "^6.0.0-beta.47",
    "@electron-forge/maker-deb": "^6.0.0-beta.47",
    "@electron-forge/maker-rpm": "^6.0.0-beta.47",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.47",
    "@electron-forge/maker-zip": "^6.0.0-beta.47",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.47",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/googlemaps": "3.30.16",
    "@types/markerclustererplus": "2.1.33",
    "@types/node": "^13.1.4",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.1",
    "electron": "^7.1.7",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-flowtype": "^4.5.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "node-loader": "^0.6.0",
    "react-hot-loader": "^4.12.18",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "monaco-editor-webpack-plugin": "^1.8.1"
  }
}
